    file_name->value());
} else if (strcmp(io_medium->text(), "serial") == 0) {
  snprintf( buf, sizeof(buf), "--%s=serial,%s,%d,%s,%s",
    io_protocol->text(), io_dir->text(), int(io_hz->value()),
