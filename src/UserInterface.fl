} else if (strcmp(io_medium->text(), "socket") == 0) {
  snprintf( buf, sizeof(buf), "--%s=socket,%s,%d,%s,%d,%s",
    io_protocol->text(), io_dir->text(), int(io_hz->value()),
    socket_host->value(), int(socket_port->value()),
    socket_tcp->value() ? "tcp" : "udp" );
}

int n = io_list->value();
if (n > 0) {
  io_list->text(n, buf );
