  Function {set_choice(Fl_Choice* c, const char* s)} {private return_type int
  } {
    code {const Fl_Menu_Item* m = c->menu();
int i = 0;
int nest = 0;
for (i = 0; i < m->size(); ++i)
{
  if (m[i].submenu())
  {
    ++nest;
  }
  else if (m[i].label() != 0)
  {
    if (strcmp(s, m[i].label()) == 0)
    {
      c->value(i);
      return 1;
    }
  }
  else
  {
    --nest;
  }
}
return 0;} {}
